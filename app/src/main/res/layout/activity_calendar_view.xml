            android:layout_height="fill_parent"
            android:layout_alignParentBottom="true"
            android:layout_below="@+id/dividerView2"
            android:fadingEdge="none"
            android:overScrollMode="never"
            android:padding="0dp"
            android:scrollbars="none">


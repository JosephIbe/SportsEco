                    <!--> Separator between Friday and Saturday <!-->
                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="@color/white" />

                    <!--> Saturday Events <!-->
                    <RelativeLayout
                        android:id="@+id/saturdayEvents"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2">

                        <include layout="@layout/calendar_events" />

                    </RelativeLayout>

                </LinearLayout>

            </RelativeLayout>

        </ScrollView>

    </LinearLayout>

</LinearLayout>